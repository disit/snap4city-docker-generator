    <!-- http://schema.org/location -->

    <ObjectProperty rdf:about="http://schema.org/location">
        <rdfs:domain rdf:resource="http://schema.org/Event"/>
        <rdfs:range rdf:resource="http://schema.org/Place"/>
        <rdfs:comment xml:lang="en">Property of the schema.org ontology.</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà dell&apos;ontologia schema.org.</rdfs:comment>
        <rdfs:label xml:lang="en">Location</rdfs:label>
        <rdfs:label xml:lang="it">Locazione</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#accessToElement -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#accessToElement">
        <inverseOf rdf:resource="http://www.disit.org/km4city/schema#hasRule"/>
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#EntryRule"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#RoadElement"/>
        <rdfs:comment xml:lang="en">An entry rule refers to a specific Road element.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una regola di accesso si riferisce ad uno specifico elemento stradale.</rdfs:comment>
        <rdfs:label xml:lang="it">Accede all&apos;elemento</rdfs:label>
        <rdfs:label xml:lang="en">Access to element</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#approvedByPa -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#approvedByPa">
        <inverseOf rdf:resource="http://www.disit.org/km4city/schema#hasResolution"/>
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#Resolution"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#Pa"/>
        <rdfs:comment xml:lang="it">Collega una delibera all&apos;ente pubblico che la ha emanata.</rdfs:comment>
        <rdfs:comment xml:lang="en">To connect a resolution to the municipality that have approved it.</rdfs:comment>
        <rdfs:label xml:lang="it">Approvato dalla PA</rdfs:label>
        <rdfs:label xml:lang="en">Approved by PA</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#arrangedOnRoad -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#arrangedOnRoad">
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#Beacon"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#Road"/>
        <rdfs:comment xml:lang="it">Collega il lampione alla strada su cui è installato.</rdfs:comment>
        <rdfs:comment xml:lang="en">To connect each beacon to the road in which it is installed.</rdfs:comment>
        <rdfs:label xml:lang="en">Arranged on road</rdfs:label>
        <rdfs:label xml:lang="it">Installato nella strada</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#atBusStop -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#atBusStop">
        <inverseOf rdf:resource="http://www.disit.org/km4city/schema#hasForecast"/>
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#BusStopForecast"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#BusStop"/>
        <rdfs:comment xml:lang="en">Each forecast refers to a specific bus stop.</rdfs:comment>
        <rdfs:comment xml:lang="it">Ogni previsione si riferisce ad una specifica fermata.</rdfs:comment>
        <rdfs:label xml:lang="it">Alla fermata</rdfs:label>
        <rdfs:label xml:lang="en">At bus stop</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#beginsAtJunction -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#beginsAtJunction">
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#RouteLink"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#RouteJunction"/>
        <rdfs:comment xml:lang="it">Ciascun tratto della linea ha una giunzione di inizio e una giunzione di fine.</rdfs:comment>
        <rdfs:comment xml:lang="en">Each routeLink, has a beginning and an end junction.</rdfs:comment>
        <rdfs:label xml:lang="en">Begins at junction</rdfs:label>
        <rdfs:label xml:lang="it">Inizia alla giunzione</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#belongToElement -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#belongToElement">
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#Entry"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#RoadElement"/>
        <rdfs:comment xml:lang="it">Questa proprietà indica il segmento stradale su cui si trova l&apos;accesso.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property indicates the segment of road where an entry point of a street number is located.</rdfs:comment>
        <rdfs:label xml:lang="it">Appartiene al segmento stradale</rdfs:label>
        <rdfs:label xml:lang="en">Belong to element</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#belongToMunicipality -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#belongToMunicipality">
        <inverseOf rdf:resource="http://www.disit.org/km4city/schema#hasDistrict"/>
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#District"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#Municipality"/>
        <rdfs:comment xml:lang="en">A municipality can be divided in districts.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un comune può essere suddiviso in frazioni o quartieri.</rdfs:comment>
        <rdfs:label xml:lang="it">Appartiene al comune</rdfs:label>
        <rdfs:label xml:lang="en">Belong to municipality</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#belongToRoad -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#belongToRoad">
        <inverseOf rdf:resource="http://www.disit.org/km4city/schema#hasStreetNumber"/>
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#StreetNumber"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#Road"/>
        <rdfs:comment xml:lang="en">Each road has street numbers.</rdfs:comment>
        <rdfs:comment xml:lang="it">Ogni strada ha i numeri civici.</rdfs:comment>
        <rdfs:label xml:lang="it">Appartiene alla strada</rdfs:label>
        <rdfs:label xml:lang="en">Belong to road</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#coincideWith -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#coincideWith">
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#Road"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#AdministrativeRoad"/>
        <rdfs:comment xml:lang="it">Non c&apos;è una corrispondenza esatta tra toponimi ed estese amministrative.</rdfs:comment>
        <rdfs:comment xml:lang="en">There is no exact match between Road and AdministrativeRoad.</rdfs:comment>
        <rdfs:label xml:lang="it">Coincide con</rdfs:label>
        <rdfs:label xml:lang="en">Coincide with</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#composeDirection -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#composeDirection">
        <inverseOf rdf:resource="http://www.disit.org/km4city/schema#consistOfElement"/>
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#RailwayElement"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#RailwayDirection"/>
        <rdfs:comment xml:lang="en">A railway Direction is composed by more railway element.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una direzione ferroviaria è formata da diversi elementi ferroviari.</rdfs:comment>
        <rdfs:label xml:lang="it">Compone direttrice</rdfs:label>
        <rdfs:label xml:lang="en">Compose direction</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#composeSection -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#composeSection">
        <inverseOf rdf:resource="http://www.disit.org/km4city/schema#isComposedByElement"/>
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#RailwayElement"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#RailwaySection"/>
        <rdfs:comment xml:lang="en">A railway Section is composed by more railway element.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una sezione ferroviaria è formata da più elementi ferroviari.</rdfs:comment>
        <rdfs:label xml:lang="it">Compone sezione</rdfs:label>
        <rdfs:label xml:lang="en">Compose section</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#concernLine -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#concernLine">
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#AVMRecord"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#PublicTransportLine"/>
        <rdfs:comment xml:lang="en">An AVMrecord concert to a unique line.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una rilevazione AVM si riferisce ad una specifica linea del TPL.</rdfs:comment>
        <rdfs:label xml:lang="en">Concern line</rdfs:label>
        <rdfs:label xml:lang="it">Riguarda la linea</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#concerningNode -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#concerningNode">
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#Maneuver"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#Node"/>
        <rdfs:comment xml:lang="en">A maneuver takes place in correspondence of a node.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una manovra è eseguita in corrispondenza di un nodo.</rdfs:comment>
        <rdfs:label xml:lang="en">Concerning node</rdfs:label>
        <rdfs:label xml:lang="it">Riguarda il nodo</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#consistOfElement -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#consistOfElement">
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#RailwayDirection"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#RailwayElement"/>
        <rdfs:comment xml:lang="en">A railway Direction is composed by more railway element.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una direzione ferroviaria è formata da più elementi ferroviari.</rdfs:comment>
        <rdfs:label xml:lang="en">Consist of element</rdfs:label>
        <rdfs:label xml:lang="it">Formato da elemento</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#containsElement -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#containsElement">
        <inverseOf rdf:resource="http://www.disit.org/km4city/schema#isPartOfRoad"/>
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#Road"/>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#RoadElement"/>
        <rdfs:comment xml:lang="en">A road is formed by more elements road.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una strada è formata da più elementi stradali.</rdfs:comment>
        <rdfs:label xml:lang="en">Contains element</rdfs:label>
        <rdfs:label xml:lang="it">Contiene elemento</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#containsSite -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#containsSite">
        <inverseOf rdf:resource="http://www.disit.org/saref4bldg-ext/isPartOfIndustrialArea"/>
        <rdfs:domain rdf:resource="http://www.disit.org/km4city/schema#IndustrialArea"/>
        <rdfs:range rdf:resource="http://www.disit.org/saref4bldg-ext/Site"/>
        <rdfs:comment xml:lang="en">An industrial area is made up of one or more industrial sites.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un&apos;area industriale è formata da più siti industriali.</rdfs:comment>
        <rdfs:label xml:lang="en">Contains industrial site</rdfs:label>
        <rdfs:label xml:lang="it">Contiene sito industriale</rdfs:label>
    </ObjectProperty>
    


    <!-- http://www.disit.org/km4city/schema#correspondToJunction -->

    <ObjectProperty rdf:about="http://www.disit.org/km4city/schema#correspondToJunction">
        <rdfs:domain>
            <Class>
                <unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.disit.org/km4city/schema#GoodsYard"/>
                    <rdf:Description rdf:about="http://www.disit.org/km4city/schema#TrainStation"/>
                </unionOf>
            </Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.disit.org/km4city/schema#RailwayJunction"/>
